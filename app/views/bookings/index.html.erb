<div class="wrapper">
  <h1 class="text-center">My bookings</h1>
  <div class="container">
    <div class="row">
      <% @bookings.each do |b| %>
        <div class="col-xs-4 col-sm-12">
          <div class="card text-center">
            <div class="card-header" style="background-image: url('http://placehold.it/350x150')">
              <div class="card-body">
              <%= link_to b.id, space_path(b) %>
              <%= b.user.email %>
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>

    <div class="row">
      <div class="message">
        <img class="avatar-square" src="">
        <div class="message-name">
          <h3>Gab Dehan</h3>
          <p>10/07/17</p>
        </div>
        <div class="message-body">
          <p><strong>I am not sure yet!</strong></p>
          <p>
            I am not sure yet! I might be in Shanghai at this moment... Can I tell you later?
          </p>
        </div>
        <div class="message-status">
          <p class="status pending">Pending</p>
          <p class="price">â‚¬345</p>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="progress">
        <div class="progress-bar" role="progressbar" style="width: 25%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">25%</div>
      </div>
    </div>


  </div>
</div>
